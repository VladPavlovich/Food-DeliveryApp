<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RestaurantScreen">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="10dp">




        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/restaurantImagesFragment"
            android:name="com.text.finalproject.RestaurantImagesFragment"
            android:layout_width="match_parent"
            android:layout_height="350dp"/>


        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/restaurantFoodItemsFragment"
            android:name="com.text.finalproject.RestaurantFoodItemsFragment"
            android:layout_width="match_parent"
            android:layout_height="350dp"/>



</LinearLayout>

</FrameLayout>